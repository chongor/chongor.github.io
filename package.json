{
    "name": "chongor.github.io",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "watch": "npm-run-all --print-label --parallel watch:scss watch:pug",
        "watch:scss": "sass --watch --poll scss:css",
        "watch:pug": "pug3 --watch pug --out ."
    },
    "devDependencies": {
        "@tokilabs/pug3-cli": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "pug": "^3.0.3",
        "sass": "^1.86.3"
    }
}
